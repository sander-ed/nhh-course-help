reset;

model burrito.mod;
data burrito.dat;
option solver gurobi;
option gurobi_options 'outlev=1';
option show_stats 1;
solve;

display profit, y, x, {(i,j) in I cross J} y[i,j] * d[i] * a[i,j] > burrito-output.txt;
display profit, y, x;


for {i in I, j in J} {
    if (y[i,j] * d[i] * a[i,j]) > 0 then { 
        printf "Demand from building %s to truck in %s: %g\n", i, j, (y[i,j] * d[i] * a[i,j]);
    }
}
